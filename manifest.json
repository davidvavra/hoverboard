{
  "name": "{$ title $}",
  "short_name": "{$ webapp.shortName $}",
  "icons": [
    {
      "src": "images/favicon/favicon-72.png",
      "sizes": "72x72",
      "type": "image/png"
    }
  ],
  "start_url": "./?utm_source=web_app_manifest",
  "display": "standalone",
  "background_color": "{$ webapp.backgroundColor $}",
  "theme_color": "{$ webapp.themeColor $}",
  "content_security_policy": "script-src 'self' https://apis.google.com https://www.googleapis.com https://securetoken.googleapis.com; object-src 'self'",
  "permissions": ["https://*/*","activeTab"]
}
